#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ADMIN/Downloads/rest-countries-api-with-color-theme-switcher-master/node_modules/mime/bin/node_modules:/mnt/c/Users/ADMIN/Downloads/rest-countries-api-with-color-theme-switcher-master/node_modules/mime/node_modules:/mnt/c/Users/ADMIN/Downloads/rest-countries-api-with-color-theme-switcher-master/node_modules:/mnt/c/Users/ADMIN/Downloads/node_modules:/mnt/c/Users/ADMIN/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/ADMIN/Downloads/rest-countries-api-with-color-theme-switcher-master/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ADMIN/Downloads/rest-countries-api-with-color-theme-switcher-master/node_modules/mime/bin/node_modules:/mnt/c/Users/ADMIN/Downloads/rest-countries-api-with-color-theme-switcher-master/node_modules/mime/node_modules:/mnt/c/Users/ADMIN/Downloads/rest-countries-api-with-color-theme-switcher-master/node_modules:/mnt/c/Users/ADMIN/Downloads/node_modules:/mnt/c/Users/ADMIN/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/ADMIN/Downloads/rest-countries-api-with-color-theme-switcher-master/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/bin/cli.js" "$@"
else
  exec node  "$basedir/../mime/bin/cli.js" "$@"
fi
